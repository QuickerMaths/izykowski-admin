{
    "name": "izykowski-admin",
    "version": "1",
    "type": "module",
    "main": "api/index.ts",
    "scripts": {
        "start": "tsx watch api/index.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "build": "rimraf dist && tsc",
        "ts.check": "tsc --project tsconfig.json",
        "add-build": "git add dist"
    },
    "pre-commit": [
        "ts.check",
        "build",
        "add-build"
    ],
    "lint-staged": {
        "*.ts": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@eslint/js": "^9.10.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/node": "^22.5.4",
        "@typescript-eslint/eslint-plugin": "^8.5.0",
        "@typescript-eslint/parser": "^8.5.0",
        "@vercel/node": "^3.2.14",
        "eslint": "^9.10.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.9.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "tsx": "^4.19.1",
        "typescript": "^5.6.2",
        "typescript-eslint": "^8.5.0"
    },
    "dependencies": {
        "@adminjs/express": "^6.1.0",
        "@adminjs/mongoose": "^4.1.0",
        "adminjs": "^7.8.12",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.0",
        "express-formidable": "^1.2.0",
        "express-session": "^1.18.0",
        "mongoose": "^8.6.2",
        "pre-commit": "^1.2.2",
        "rimraf": "^6.0.1",
        "tslib": "^2.7.0"
    }
}
